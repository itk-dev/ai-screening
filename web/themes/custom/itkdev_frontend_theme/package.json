    "tailwindcss": "^3.4.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "coding-standards-check/eslint": "yarn eslint './js/**/*.js'",
    "coding-standards-check/prettier": "yarn prettier ./js/**/ --check",
    "coding-standards-check": "yarn coding-standards-check/prettier && yarn coding-standards-check/eslint",
    "coding-standards-apply/eslint": "yarn eslint './js/**/*.js' --fix",
    "coding-standards-apply/prettier": "yarn prettier ./js/**/ --write",
    "coding-standards-apply": "yarn coding-standards-apply/prettier && yarn coding-standards-apply/eslint",
    "build": "yarn encore production --progress && yarn tailwindcss -i styles.css -o build/styles.css --minify",
    "watch": "yarn encore dev --watch & yarn tailwindcss -i styles.css -o build/styles.css --watch"
  }
